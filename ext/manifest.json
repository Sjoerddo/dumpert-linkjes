{
  "name": "Dumpert Linkjes",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/background/index.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Linkjes",
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://dumpert.nl/mediabase/*"
  ]
}